        "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.zhangnx.echarts.dao.EchartsDao">

    <resultMap id="BaseResultMap" type="com.zhangnx.echarts.pojo.Echarts" >
        <id column="id" property="id"  />
